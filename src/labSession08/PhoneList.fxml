<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="labSession08.PhoneList">
    <AnchorPane prefHeight="400.0" prefWidth="600.0">
        <TableView fx:id="tableView" prefHeight="306.0" prefWidth="550.0" AnchorPane.bottomAnchor="69.0"
                   AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
            <columns>
                <TableColumn fx:id="accountId" prefWidth="67.0" text="Account id"/>
                <TableColumn fx:id="name" prefWidth="189.0" text="Name"/>
                <TableColumn fx:id="workplace" prefWidth="133.0" text="Workplace"/>
                <TableColumn fx:id="address" prefWidth="159.0" text="Address"/>
            </columns>
        </TableView>
        <ListView fx:id="lvName" layoutX="92.0" layoutY="50.0" onMouseClicked="#getPhone" prefHeight="280.0"
                  prefWidth="190.0"/>
        <Button onAction="#addAccount" layoutX="66.0" layoutY="351.0" mnemonicParsing="false" text="Add"/>
        <Button onAction="#deleteAccount" layoutX="135.0" layoutY="351.0" mnemonicParsing="false" text="Delete"/>
        <Text fx:id="message" layoutX="407.0" layoutY="368.0" strokeType="OUTSIDE" strokeWidth="0.0"
              AnchorPane.rightAnchor="50.0"/>
    </AnchorPane>
</Pane>
